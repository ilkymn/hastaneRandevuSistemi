@model hastaneRandevuSistemi.Models.signUp.signUp

<form asp-controller="signUp" asp-action="Create">



    <div class="form-group">
        <label asp-for="patientName"> </label>
        <input asp-for="patientName" />
        <span asp-validation-for="patientName"></span>

    </div>
    <div class="form-group">
        <label asp-for="patientSurname"> </label>
        <input asp-for="patientSurname" />
        <span asp-validation-for="patientSurname"></span>

    </div>

    <div class="form-group">
        <label asp-for="patientBirthday"> </label>
        <input asp-for="patientBirthday" />
        <span asp-validation-for="patientBirthday"></span>

    </div>

    <div class="form-group">
        <label asp-for="patientPhoneNo"> </label>
        <input asp-for="patientPhoneNo" />
        <span asp-validation-for="patientPhoneNo"></span>

    </div>

    <div class="form-group">
        <label asp-for="patientEmail"> </label>
        <input asp-for="patientEmail" />
        <span asp-validation-for="patientEmail"></span>

    </div>



    <div class="form-group" typeof="password">
        <label asp-for="patientParola"> </label>
        <input asp-for="patientParola" />
        <span asp-validation-for="patientParola"></span>

    </div>

    <div class="form-group" typeof="password">
        <label asp-for="patientTeParola"> </label>
        <input asp-for="patientTeParola" />
        <span asp-validation-for="patientTeParola"></span>

    </div>

    <div class="form-group">
        <label asp-for="patientCity"> </label>
        <input asp-for="patientCity" />
        <span asp-validation-for="patientCity"></span>

    </div>


    <div class="form-group">
        <input type="submit" value="Create" asp-controller="login" asp-action="Index" />


    </div>

</form>

